<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="trackingOrder" type="TrackingOrderType"/>

  <xs:complexType name="main">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="TrackingOrderType">
    <xs:sequence>
      <xs:element name="deliveryDate" type="xs:anySimpleType" minOccurs="0"/>
      <xs:element name="orderDate" type="xs:anySimpleType" minOccurs="0"/>
      <xs:element name="shipDate" type="xs:date" minOccurs="0"/>
      <xs:element name="trackingDuration" type="xs:duration" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

